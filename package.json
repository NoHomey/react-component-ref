{
  "name": "react-component-ref",
  "version": "1.0.1",
  "description": "Simple helper class to help you stay DRY (Don't repeat yourself)",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "tsc && jest",
    "typings": "typings install",
    "npm": "tsc -d -p . && mv ./src/index.js . && mv ./src/index.d.ts .",
    "clean": "rm -f ./src/index.js ./__tests__/*.d.ts ./__tests__/*.js ./__fixtures__/*.d.ts ./__fixtures__/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoHomey/react-component-ref.git"
  },
  "keywords": [
    "react",
    "component",
    "ref",
    "helper",
    "class",
    "dry"
  ],
  "author": "Ivo Stratev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NoHomey/react-component-ref/issues"
  },
  "homepage": "https://github.com/NoHomey/react-component-ref#readme",
  "dependencies": {
    "bind-decorator": "^1.0.3"
  },
  "devDependencies": {
    "enzyme": "^2.4.1",
    "jest": "^15.1.1",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.3"
  },
  "jest": {
    "verbose": true
  }
}
